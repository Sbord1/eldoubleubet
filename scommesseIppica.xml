        <id>1</id>
        <scommettitore>matteo</scommettitore>
        <risultato>
            <primo>5</primo>
            <secondo>2</secondo>
            <terzo>1</terzo>
        </risultato>
        <puntata>5</puntata>
    </scommessa>

    <scommessa eliminato="0">
        <id>1</id>
        <scommettitore>andrea</scommettitore>
